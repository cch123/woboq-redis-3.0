<dec f='redis/deps/hiredis/sds.h' l='81' type='void sdsrange(sds s, int start, int end)'/>
<use f='redis/deps/hiredis/hiredis.c' l='653' u='c' c='redisReaderGetReply'/>
<use f='redis/deps/hiredis/hiredis.c' l='1192' u='c' c='redisBufferWrite'/>
<def f='redis/deps/hiredis/sds.c' l='598' ll='626' type='void sdsrange(sds s, int start, int end)'/>
<doc f='redis/deps/hiredis/sds.c' l='582'>/* Turn the string into a smaller (or equal) string containing only the
 * substring specified by the &apos;start&apos; and &apos;end&apos; indexes.
 *
 * start and end can be negative, where -1 means the last character of the
 * string, -2 the penultimate character, and so forth.
 *
 * The interval is inclusive, so the start and end characters will be part
 * of the resulting string.
 *
 * The string is modified in-place.
 *
 * Example:
 *
 * s = sdsnew(&quot;Hello World&quot;);
 * sdsrange(s,1,-1); =&gt; &quot;ello World&quot;
 */</doc>
<dec f='redis/src/sds.h' l='81' type='void sdsrange(sds s, int start, int end)'/>
